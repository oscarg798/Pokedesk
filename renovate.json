{
    "extends": [
        "config:base"
    ],
    "groupName": "all",
    "separateMajorMinor": false,
    "packageRules": [{
            "matchPackagePatterns": [
                "*"
            ],
            "groupName": "all"
        },
        {
            "matchPackagePatterns": [
                "^androidx\\.navigation.*$"
            ],
            "groupName": "Navigation"
        },
        {
            "matchPackagePatterns": [
                "^org\\.jetbrains\\.kotlin:.*$",
                "^.*services.gradle.org.*$",
                "^androidx\\.compose.*$"
            ],
            "enabled": false
        },
        {
            "matchPaths": [
                "gradle-wrapper.properties"
            ],
            "enabled": false
        }
    ],
    "npm": {
        "schedule": "at any time"
    },
    "timezone": "Europe/Madrid",
    "schedule": [
        "at any time"
    ],
    "rebaseWhen": "behind-base-branch"
}
